<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bazo Explorer</title>
    <link rel="stylesheet" href="/static/css/bootstrap.css">
    <!-- <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet"> -->
  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Transaction {{.Hash}}</h3>
        </div>
        <div class="data-table" id="transactions-table">
          <table class="table">
            <thead>
              <tr>
                <th>Hash</th>
                <td> <a href="/transactions/fundtx/{{.Hash}}">{{.Hash}}</a></td>
              </tr>
              <tr>
                <th>From</th>
                <td> <a href="/account/{{.From}}">{{.From}}</a></td>
              </tr>
              <tr>
                <th>Transaction Count</th>
                <td>{{.TxCount}}</td>
              </tr>
              <tr>
                <th>To</th>
                <td> <a href="/account/{{.To}}">{{.To}}</a></td>
              </tr>
              <tr>
                <th>Amount</th>
                <td>{{.Amount}}</td>
              </tr>
                <th>Fee</th>
                <td>{{.Fee}}</td>
              </tr>
              <tr>
                <th>Signature</th>
                <td>{{.Signature}}</td>
              </tr>
            </thead>

          </table>
        </div>
      </div>
    </section>

  </body>
</html>
