<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bazo Explorer</title>
    <link rel="stylesheet" href="/static/css/bootstrap.css">
    <!-- <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet"> -->
  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Account {{.Address}}</h3>
        </div>
        <div class="data-table">
          <table class="table">
            <thead>
              <tr>
                <th>Address</th>
                <td>{{.Address}}</td>
              </tr>
              </tr>
                <th>Balance</th>
                <td>{{.Balance}}</td>
              </tr>
              </tr>
                <th>Transaction Count</th>
                <td>{{.TxCount}}</td>
              </tr>
            </thead>
          </table>
        </div>
      </div>
    </section>

    <div class="clr"></div>

  </body>
</html>
