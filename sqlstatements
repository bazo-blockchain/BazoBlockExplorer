db.createUser({
  user:"testuser",
  pwd:"1234",
  roles:["readWrite", "dbAdmin"]
})

create database BlockExplorerTest1;

create table blocks (
header bit(8),
hash char(32) primary key,
prevHash char(32) unique not null,
nonce char(8) not null,
timestamp timestamp not null,
merkleRoot char(32) not null,
beneficiary char(32) not null,
nrFundTx smallint not null,
nrAccTx smallint not null,
nrConfigTx smallint not null,
fundsTxData char(32)[],
accTxData char(32)[],
configTxData char(32)[]
)

