<!DOCTYPE html>
<html>

  <head>

    {{template "head"}}

  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Transaction {{.Hash}}</h3>
        </div>
        <div class="data-table" id="transactions-table">
          <table class="table">
            <thead>
              <tr>
                <th style="width: 25%;">Hash</th>
                <td> <a href="/bazo/transactions/acc/{{.Hash}}">{{.Hash}}</a></td>
              </tr>
              <tr>
                <th>Block</th>
                <td> <a href="/bazo/block/{{.BlockHash}}">{{.BlockHash}}</a></td>
              </tr>
              <tr>
                <th>Issuer</th>
                <td> <a href="/bazo/account/{{.Issuer}}">{{.Issuer}}</a></td>
              </tr>
              <tr>
                <th>New PubKey</th>
                <td> <a href="/bazo/account/{{.PubKey}}">{{.PubKey}}</a></td>
              </tr>
                <th>Fee</th>
                <td>{{.Fee}}</td>
              </tr>
              <tr>
                <th>Signature</th>
                <td>{{.Signature}}</td>
              </tr>
            </thead>

          </table>
        </div>
      </div>
    </section>

    {{template "scriptImports"}}

  </body>
</html>
