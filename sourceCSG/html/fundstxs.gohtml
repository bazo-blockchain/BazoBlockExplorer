<!DOCTYPE html>
<html>

  <head>

    {{template "head"}}

  </head>

  <body>

    {{template "navBar"}}

    <div class="clr"></div>

    <section>
      <div class="container">
        <div class="table-title">
          <h3>Latest Funds Transactions</h3>
        </div>
        <div class="data-table" id="transactions-table">
          <table class="table">
            <thead>
              <tr id="header-row">
                <th>Hash</th>
                <th>From</th>
                <th>To</th>
                <th style="width: 10%;">Amount</th>
                <th style="width: 10%;">Fee</th>
              </tr>
            </thead>
            <tbody>
              {{range .}}
              <tr>
                <td> <a href="/bazo/transactions/funds/{{.Hash}}">{{.Hash}}</a></td>
                <td> <a href="/bazo/account/{{.From}}">{{.From}}</a></td>
                <td> <a href="/bazo/account/{{.To}}">{{.To}}</a></td>
                <td>{{.Amount}}</td>
                <td>{{.Fee}}</td>
              </tr>
              {{end}}
            </tbody>
          </table>
        </div>
      </div>
    </section>

    {{template "scriptImports"}}

  </body>
</html>
